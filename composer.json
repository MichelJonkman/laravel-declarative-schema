{
    "name": "micheljonkman/laravel-declarative-schema",
    "version": "1.2.0",
    "description": "",
    "keywords": [
        "MichelJonkman",
        "laravel-director",
        "laravel-dbal-schema"
    ],
    "homepage": "https://github.com/micheljonkman/laravel-declarative-schema",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Jonkman",
            "email": "mj.99@live.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "micheljonkman/declarative-schema": "^1.2.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "MichelJonkman\\LaravelDeclarativeSchema\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelJonkman\\LaravelDeclarativeSchema\\Providers\\SchemaServiceProvider",
                "MichelJonkman\\LaravelDeclarativeSchema\\Providers\\ConsoleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
